</head>

<body>


    @Html.Partial("_Preloader")
    @Html.Partial("_navbar")
    <div class="page-heading">
        <div class="container">
            <div class="row">
                <div class="col-lg-8">
                    <div class="top-text header-text">
                        <h6>Check Out Our Listings</h6>
                        <h2>Item listings of Different Categories</h2>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="listing-page">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="naccs">
                        <div class="grid">
                            <div class="row">
                                <div class="col-lg-3">
                                    <div class="menu">
                                        <div class="first-thumb active">
                                            <div class="thumb">
                                                <span class="icon"><img src="~/Template/assets/images/search-icon-01.png" alt=""></span>
                                                Apartments
                                            </div>
                                        </div>
                                        <div>
                                            <div class="thumb">
                                                <span class="icon"><img src="~/Template/assets/images/search-icon-02.png" alt=""></span>
                                                Office
                                            </div>
                                        </div>
                                        <div>
                                            <div class="thumb">
                                                <span class="icon"><img src="~/Template/assets/images/search-icon-03.png" alt=""></span>
