                                                                            <a href="#"><h4>Traveling Item 1</h4></a>
                                                                            <h6>by: Travel Agent</h6>
                                                                            <span class="price"><div class="icon"><img src="~/Template/assets/images/listing-icon-01.png" alt=""></div> $400 / month included tax</span>
                                                                            <span class="details">Details: <em>860 sq ft</em></span>
                                                                            <span class="info"><img src="~/Template/assets/images/listing-icon-02.png" alt=""> 2 Bedrooms<br><img src="~/Template/assets/images/listing-icon-03.png" alt=""> 3 Bathrooms</span>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            }
                                                        </div>
                                                    </div>   
                                                </div>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @Html.Partial("_Footer")

    <script src="~/Template/vendor/jquery/jquery.min.js"></script>
    <script src="~/Template/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Template/assets/js/owl-carousel.js"></script>
    <script src="~/Template/assets/js/animation.js"></script>
